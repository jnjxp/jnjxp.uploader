{
    "name": "jnjxp/uploader",
    "description": "Utilities for file upload interface",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "jake johns",
            "email": "jake@jakejohns.net"
        }
    ],
    "config": {
        "sort-packages": true
    },
    "extra": {
        "zf": {
            "config-provider": [
                "Jnjxp\\Uploader\\ConfigProvider"
            ]
        }
    },
    "require": {
        "fig/http-message-util": "^1.1",
        "jnjxp/filed": "0.4.0",
        "psr/container": "^1.0",
        "psr/http-factory": "^1.0",
        "psr/http-message": "^1.0",
        "psr/http-message-implementation": "^1.0",
        "psr/http-server-handler": "^1.0",
        "mezzio/mezzio": "^3.2",
        "laminas/laminas-dependency-plugin": "^1.0"
    },
    "require-dev": {
        "pds/skeleton": "~1.0",
        "phpunit/phpunit": "^8.0",
        "laminas/laminas-diactoros": "^2.1"
    },
    "autoload": {
        "psr-4": {
            "Jnjxp\\Uploader\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Jnjxp\\Uploader\\": "tests/"
        }
    }
}
